    <form asp-controller="Limpiador" asp-action="grabar" method="post">
        Nombre:<input type="text" asp-for="NomLimp" placeholder="Ingrese el nuevo Nombre del Limpiador" />
        Apellido:<input type="text" asp-for="ApeLimp" placeholder="Ingrese el nuevo Apellido del Limpiador" />
        Dirección:<input type="text" asp-for="DirLimp" placeholder="Ingrese la nueva dirección del Limpiador" />
        Celular:<input type="text" asp-for="TelLimp" placeholder="Ingresar el nuevo numero de telefono del Limpiador" />
        Disponibilidad:<input type="text" asp-for="DisLimp" placeholder="Ingrese el nuevo Disponibilidad" />
        Tarifa:<input type="number" asp-for="TarLimp" placeholder="Ingrese el nuevo Precio" />
        <input type="submit" value="Grabar" />
